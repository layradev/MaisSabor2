@model MaisSabor2.ViewModel.HomeViewMoldel
@{
    ViewData["Title"] = "Cardápio";
}
<partial name="_Carousel" />
<h1 class="my-3 text-center">Itens em Destaque</h1>
<div class="row row-cols-1 row-cols-sm-3">
    @foreach (var item in Model.ItensEmDestaque)
    {
        <partial name="_Card" for="@item"></partial>
    }
</div>
